// Compute NDVI.
var ndvipost = image.normalizedDifference(['b5', 'b4']);
var ndvipre = image2.normalizedDifference(['b4', 'b3']);
// print(ndvipost);
// print(ndvipre);
Map.addLayer(ndvipost)
Map.addLayer(ndvipre)
