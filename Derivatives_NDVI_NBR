// Compute NBR.
var nbrpre = image2.select('b3').subtract(image2.select("b7")).divide
              (image2.select('b3').add(image2.select("b7")));

var nbrpost = image.select('b4').subtract(image.select("b7")).divide
              (image.select('b4').add(image.select("b7")));
